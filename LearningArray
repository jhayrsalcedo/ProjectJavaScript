<!DOCTYPE html>
<html lang="en">
<head>
    <script>
        var scores = [60, 50, 60, 58, 54, 54,     
                    58, 50, 52, 54, 48, 69,
                    34, 55, 51, 52, 44, 51,
                    69, 64, 66, 55, 52, 61,
                    46, 31, 57, 52, 44, 18,
                    41, 53, 55, 61, 51, 44];

        var highScore = 0;
        var bestSolution = [];
        var output;
    

        for (var i = 0; i < scores.length; i++) {
            output = "Bubbles solution #" + i + " score:" + scores[i];
            console.log(output);

            if( scores[1] > highScore){
                highScore = scores[i];
            }
        }

        var bestSolutions = [];
        for (var i = 0; i < scores.length; i++){
            if( scores[i] == highScore){
                bestSolutions.push(i);
            }
        }
       

        
        console.log("Bubble test: " + scores.length);
        console.log("Highest score: " + highScore);
        console.log("Solution with the best score: " + bestSolutions );
    </script>
        
    <title>Bubble Tea</title>
</head>
<body>
    
</body>
</html>
